<!DOCTYPE html>
<html>
<head>
	<title>Delete Doctor</title>
</head>
<body>
	<form method="post">
		<fieldset>
			<legend>Delete Doctor</legend>
			<table>
				<tr>
					<td>Id:</td>
					<td><%=id%></td>
				</tr>
				<tr>
					<td>first name:</td>
					<td><%=fname%></td>
				</tr>
				<tr>
					<td>Last name:</td>
					<td><%=lname%></td>
				</tr>
				<tr>
					<td>Username :</td>
					<td><%=username%></td>
				</tr>
				<tr>
					<td>Password :</td>
					<td><%=password%></td>
				</tr>
				<tr>
					<td>Email :</td>
					<td><%=email%></td>
				</tr>
				<tr>
					<td>phone:</td>
					<td><%=phone%></td>
				</tr>
				<tr>
					<td> Blood Group:</td>
					<td><%=bloodgroup%></td>
				</tr>
				<tr>
					<td>Dept :</td>
					<td><%=dept%></td>
				</tr>
				<tr>
					<td>Post:</td>
					<td><%=post%></td>
				</tr>
			</table>
			<div>
				<h3>Are you sure?</h3>
				<input type="submit" name="submit" value="Confirm">
			</div>
		</fieldset>
	</form>
</body>
</html>